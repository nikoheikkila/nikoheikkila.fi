        "@types/inquirer": "8.2.0",
        "@types/title": "3.4.1",
        "alex": "10.0.0",
        "ava": "4.0.1",
        "husky": "7.0.4",
        "inquirer": "8.2.0",
        "pa11y-ci": "2.4.2",
        "playwright": "1.19.0",
        "prettier": "2.5.1",
        "title": "3.4.3",
        "ts-node": "10.5.0"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "new": "ts-node --script-mode new.ts",
        "clean": "gatsby clean",
        "build": "gatsby build --log-pages --write-to-file",
        "dev": "gatsby develop --verbose",
        "format": "prettier --write .",
        "lint": "yarn lint:style && yarn lint:types && yarn lint:lang",
        "lint:style": "prettier --check .",
        "lint:lang": "alex -q content",
        "lint:types": "tsc --noEmit",
        "start": "yarn dev",
        "serve": "gatsby serve -p 8000",
        "test": "yarn format && yarn test:unit && yarn test:e2e && yarn test:a11y",
        "test:unit": "ava",
        "test:e2e": "playwright test",
        "test:a11y": "pa11y-ci",
        "snyk-protect": "snyk protect",
        "prepare": "yarn run snyk-protect"
    },
    "ava": {
        "require": [
            "ts-node/register"
        ],
        "files": [
            "src/__tests__/unit/*.test.ts"
        ],
        "extensions": [
            "ts"
        ],
        "concurrency": 5,
        "verbose": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nikoheikkila/nikoheikkila.fi"
    },
    "bugs": {
        "url": "https://github.com/nikoheikkila/nikoheikkila.fi/issues"
    },
    "snyk": true
}
