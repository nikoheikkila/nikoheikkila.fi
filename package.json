    "@types/react-burger-menu": "^2.6.1",
    "@types/react-helmet": "^5.0.14",
    "chalk": "^4.0.0",
    "dayjs": "^1.8.18",
    "disqus-react": "^1.0.7",
    "esm": "^3.2.25",
    "gatsby": "^2.20.26",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-disqus": "^1.1.4",
    "gatsby-plugin-feed": "^2.4.3",
    "gatsby-plugin-google-analytics": "^2.2.4",
