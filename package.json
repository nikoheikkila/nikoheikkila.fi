  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@sindresorhus/slugify": "^0.9.1",
    "chalk": "^2.4.2",
    "dayjs": "^1.8.15",
    "disqus-react": "^1.0.6",
    "esm": "^3.2.25",
    "gatsby": "^2.13.32",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-disqus": "^1.1.2",
    "gatsby-plugin-feed": "^2.3.6",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-manifest": "^2.2.5",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sass": "^2.1.4",
    "gatsby-plugin-sharp": "^2.2.11",
    "gatsby-plugin-sitemap": "^2.2.6",
    "gatsby-remark-copy-linked-files": "^2.1.5",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-prismjs": "^3.3.4",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-remark": "^2.6.10",
    "gatsby-transformer-sharp": "^2.2.4",
    "inquirer": "^6.5.0",
    "js-yaml": "^3.13.1",
    "node-sass": "^4.12.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-burger-menu": "^2.6.10",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "title": "^3.4.1"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "cypress": "^3.4.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gatsby-cypress": "^0.2.3",
    "husky": "^3.0.2",
    "pa11y-ci": "^2.3.0",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.9.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "new": "node new.js",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "eslint --fix .",
    "lint": "eslint .",
    "start": "yarn dev",
    "serve": "gatsby serve -p 8000",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "pa11y": "pa11y-ci -s http://localhost:8000/sitemap.xml -f https://nikoheikkila.fi -r http://localhost:8000",
    "test:unit": "ava",
    "test:a11y": "start-server-and-test serve http://localhost:8000 pa11y",
    "test:dev": "start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e": "start-server-and-test serve http://localhost:8000 cy:run",
    "now-build": "yarn build",
    "now-dev": "yarn dev"
  },
  "resolutions": {
    "sharp": "0.22.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "ava": {
    "files": [
      "src/__tests__/**/*.js"
    ],
    "require": [
      "esm"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi"
  },
  "bugs": {
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi/issues"
  }
}
