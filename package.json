    "use-dark-mode": "^2.3.1",
    "snyk": "^1.414.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "alex": "^9.0.1",
    "ava": "^3.12.1",
    "babel-eslint": "^10.0.3",
    "cypress": "^5.1.0",
    "eslint": "7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "1.0.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby-cypress": "^0.4.10",
    "husky": "^4.3.0",
    "pa11y-ci": "^2.4.0",
    "prettier": "^2.1.1",
    "start-server-and-test": "^1.11.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "new": "node new.js",
    "build": "gatsby build --log-pages --write-to-file",
    "dev": "gatsby develop",
    "format": "eslint --fix .",
    "lint:js": "eslint .",
    "lint:lang": "alex -q content",
    "lint": "yarn lint:js && yarn lint:lang",
    "start": "yarn dev",
    "serve": "gatsby serve -p 8000",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "pa11y": "pa11y-ci -s http://localhost:8000/sitemap.xml -f https://nikoheikkila.fi -r http://localhost:8000",
    "test": "yarn lint && yarn test:unit && yarn test:e2e && yarn test:a11y",
    "test:unit": "ava",
    "test:a11y": "start-server-and-test serve http://localhost:8000 pa11y",
    "test:dev": "cypress install && start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e": "cypress install && start-server-and-test serve http://localhost:8000 cy:run",
    "now-build": "yarn build",
    "now-dev": "yarn dev",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "ava": {
    "babel": {
      "compileEnhancements": false
    },
    "extensions": [
      "ts"
    ],
    "files": [
      "src/__tests__/**/*.ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi"
  },
  "bugs": {
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi/issues"
  },
  "snyk": true
}
