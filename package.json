    "gatsby-transformer-sharp": "^2.3.5",
    "global": "^4.4.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "node-sass": "^4.13.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-burger-menu": "^2.6.12",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "title": "^3.4.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.148",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "alex": "^8.0.1",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "cypress": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-cypress": "^0.2.16",
    "husky": "^3.0.9",
    "pa11y-ci": "^2.3.0",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "new": "node new.js",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "eslint --fix .",
    "lint:js": "eslint .",
    "lint:lang": "alex -q src/pages",
    "lint": "yarn lint:js && yarn lint:lang",
    "start": "yarn dev",
    "serve": "gatsby serve -p 8000",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "pa11y": "pa11y-ci -s http://localhost:8000/sitemap.xml -f https://nikoheikkila.fi -r http://localhost:8000",
    "test:unit": "ava",
    "test:a11y": "start-server-and-test serve http://localhost:8000 pa11y",
    "test:dev": "cypress install && start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e": "cypress install && start-server-and-test serve http://localhost:8000 cy:run",
    "now-build": "yarn build",
    "now-dev": "yarn dev"
  },
  "resolutions": {
    "sharp": "0.22.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "src/__tests__/**/*.ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi"
  },
  "bugs": {
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi/issues"
  }
}