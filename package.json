    "js-yaml": "^3.13.1",
    "katex": "^0.11.1",
    "node-sass": "^4.14.0",
    "preact": "^10.4.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-burger-menu": "^2.6.13",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "title": "^3.4.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "alex": "^8.1.0",
    "ava": "^3.7.1",
    "babel-eslint": "^10.0.3",
    "cypress": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gatsby-cypress": "^0.4.0",
    "husky": "^4.2.5",
    "pa11y-ci": "^2.3.0",
    "prettier": "^2.0.4",
    "start-server-and-test": "^1.11.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "new": "node new.js",
    "build": "gatsby build --log-pages --write-to-file",
    "dev": "gatsby develop --https",
    "format": "eslint --fix .",
    "lint:js": "eslint .",
    "lint:lang": "alex -q src/pages",
    "lint": "yarn lint:js && yarn lint:lang",
    "start": "yarn dev",
    "serve": "gatsby serve -p 8000",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "pa11y": "pa11y-ci -s http://localhost:8000/sitemap.xml -f https://nikoheikkila.fi -r http://localhost:8000",
    "test:unit": "ava",
    "test:a11y": "start-server-and-test serve http://localhost:8000 pa11y",
    "test:dev": "cypress install && start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e": "cypress install && start-server-and-test serve http://localhost:8000 cy:run",
    "now-build": "yarn build",
    "now-dev": "yarn dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "ava": {
    "babel": {
      "compileEnhancements": false
    },
    "extensions": [
      "ts"
    ],
    "files": [
      "src/__tests__/**/*.ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi"
  },
  "bugs": {
    "url": "https://github.com/nikoheikkila/nikoheikkila.fi/issues"
  }
}
