    "chalk": "^4.1.0",
    "dayjs": "^1.8.35",
    "disqus-react": "^1.0.10",
    "esm": "^3.2.25",
    "gatsby": "^2.24.58",
    "gatsby-image": "^2.4.18",
    "gatsby-plugin-disqus": "^1.2.2",
    "gatsby-plugin-feed": "^2.5.12",
    "gatsby-plugin-google-analytics": "^2.3.14",
    "gatsby-plugin-manifest": "^2.4.29",
    "gatsby-plugin-offline": "^3.2.28",
